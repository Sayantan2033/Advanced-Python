    "def func():\n",
    "    print(\"Calling function.\")\n",
    "\n",
    "def log_calls(func):\n",
    "    def wrapper():\n",
    "        name = func.__name__\n",
    "        print(f\"before {name} was called\")\n",
    "        func()\n",
    "        print(f\"after {name} was called\")\n",
    "        return wrapper\n",
    "    \n",
    "func = log_calls(func)\n",
    "\n",
    "func()"
